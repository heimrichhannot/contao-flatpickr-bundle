{
  "name": "heimrichhannot/contao-flatpickr-bundle",
  "type": "contao-bundle",
  "description": "A bundle to provide flatpickr support for contao.",
  "license": "LGPL-3.0-or-later",
  "require": {
    "php": "^7.4 || ^8.0",
    "contao/core-bundle": "^4.9",
    "heimrichhannot/contao-utils-bundle": "^2.206.0",
    "heimrichhannot/contao-twig-support-bundle": "^0.2.16 || ^1.0",
    "symfony/event-dispatcher-contracts": "^1.0 || ^2.0 || ^3.0"
  },
  "autoload": {
    "psr-4": {
      "HeimrichHannot\\FlatpickrBundle\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "HeimrichHannot\\FlatpickrBundle\\ContaoManager\\Plugin",
    "foxy": true
  }
}